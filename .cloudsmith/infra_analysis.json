      "configuration": {
        "addon_version": "v1.5.1-eksbuild.1"
      },
      "security": {
        "service_account_role": "AmazonCloudWatchObservabilityAddonRole-${var.cluster_name}",
        "policies": ["CloudWatchAgentServerPolicy", "AWSXrayFullAccess"]
